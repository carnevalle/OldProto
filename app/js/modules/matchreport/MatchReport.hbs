
<div class="matchreport type1">

    <div class="top">
        <div class="row">
            <div class="col-sm-12 time-container">

                <div id="time-display" class="time-display"><span></span>&prime;</div>

                <div class="time-left-wrapper">
                    <div class="time-controls-wrapper">
                        <button id="btn-play"type="button" class="btn btn-default"><span class="glyphicon glyphicon-play"></span></button>
                        <button id="btn-pause"type="button" class="btn btn-default hidden"><span class="glyphicon glyphicon-pause"></span></button>
                    </div>
                    <div class="time-bar-wrapper">
                        <div class="noUiSlider"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="middle">

        <div id='slider' class='swipe'>
          <div class='swipe-wrap'>
            <div><img src="http://lorempixel.com/400/400/sports/1/" style="width: 100%"/></div>
            <div><img src="http://lorempixel.com/400/400/sports/2/" style="width: 100%"/></div>
            <div><img src="http://lorempixel.com/400/400/sports/3/" style="width: 100%"/></div>
          </div>
        </div>

    </div>
    <div class="bottom">

    </div>
</div>

